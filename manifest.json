{
  "manifest_version": 2,
  "name": "Imdb ListTo Trakt",
  "description": "Load Imdb List to Trakt",
  "version": "1.0",
  "permissions": [
    "http://trakt.tv/",
    "http://imdb.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.imdb.com/search/*",
        "http://api.trakt.tv/*"
      ],
      "js": [
        "jquery-2.0.0.min.js",
        "imdb-to-trakt.js"
      ]
    }
  ]
}